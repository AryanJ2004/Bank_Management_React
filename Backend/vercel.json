{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/dist/favicon.ico"
    },
    {
      "src": "^/(.*)\\.(?:js|css|svg|png|jpg|jpeg|gif|ico)$",
      "dest": "/dist/$1.$2"
    },
    {
      "src": "^/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}